#: src/gui/uifiles/s_pcc_dialog.ui:1274 src/gui/uifiles/s_pcc_dialog.ui:1292
#: src/gui/uifiles/settings_window.ui:518
#: src/gui/uifiles/settings_window.ui:533
msgid "0"
msgstr "0"

#: src/gui/uifiles/aavso_dialog.ui:328
msgid "Check star: "
msgstr "Проверочная звезда: "

#: src/gui/uifiles/aavso_dialog.ui:339
msgid "The number of the check star."
msgstr "Номер проверочной звезды."

#: src/gui/uifiles/aavso_dialog.ui:351
msgid ""
"Please use the sequence ID you will find written in Bold print near the top "
"of the photometry table in a sentence that reads \"Report this sequence as "
"[ID] in the chart field of your observation report.\" If you used your own "
"comparison stars (e.g. in the case of time-sensitive alerts when the "
"Sequence Team had no time to create a sequence), do not give a chart ID, "
"even if you plotted the chart using VSP. Use the comment code K (non-AAVSO "
"chart) and give a proper chart name like \"APASS DR10\". Then add "
"information on the comp stars in the notes. Limit: 20 characters."
msgstr ""
"Пожалуйста, используйте идентификатор последовательности, который выделен "
"жирным шрифтом в верхней части таблицы фотометрии, в предложении следующего "
"содержания: «Укажите эту последовательность как [ID] в поле диаграммы вашего "
"отчёта о наблюдениях». Если вы использовали свои собственные звёзды "
"сравнения (например, в случае срочных предупреждений, когда у команды "
"Sequence не было времени на создание последовательности), не указывайте "
"идентификатор диаграммы, даже если вы построили диаграмму с помощью VSP. "
"Используйте код комментария K (диаграмма, отличная от AAVSO) и дайте "
