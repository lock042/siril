msgid "GZIP1"
msgstr "GZIP1"

#: src/gui/siril3.glade:28333
msgid "GZIP2"
msgstr "GZIP2"

#: src/gui/siril3.glade:28347
msgid "Method"
msgstr "Метод"

#: src/gui/siril3.glade:28361
msgid "Quantization level"
msgstr "Рівень квантування"

#: src/gui/siril3.glade:28374
