#, fuzzy
msgid "current working directory is invalid"
msgstr "Canvia el directori de treball"

#: src/core/command_line_processor.c:58
msgid "command cannot be used in a script"
msgstr ""

#: src/core/command_line_processor.c:60
#, fuzzy
msgid "invalid number of arguments"
msgstr "Regla de terços"

#: src/core/command_line_processor.c:62
msgid "invalid arguments"
msgstr ""

