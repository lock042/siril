#: src/gui/siril3.glade:2624
msgid "Number of wavelet layers that will be used"
msgstr "Número de capas wavelet que se usuarán"

#: src/gui/siril3.glade:2648 src/gui/siril3.glade:2672
msgid "Compute wavelets"
msgstr "Procesar wavelets"

#: src/gui/siril3.glade:2723
msgid "1st Plan"
msgstr "1er plano"

#: src/gui/siril3.glade:2792
msgid "2nd Plan"
msgstr "2º plano"

#: src/gui/siril3.glade:2861
msgid "3th Plan"
msgstr "3er plano"

#: src/gui/siril3.glade:2930
msgid "4th Plan"
msgstr "4º plano"

#: src/gui/siril3.glade:2999
msgid "5th Plan"
