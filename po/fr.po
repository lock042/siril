msgid "Amount"
msgstr "Quantité"

#: src/gui/siril3.glade:387
msgid ""
"0: No correction\n"
"1 : 100% corrected"
msgstr ""
"0 : Aucune correction\n"
"1 : 100% corrigés"

#: src/gui/siril3.glade:403
msgid "CFA"
msgstr "CFA"

#: src/gui/siril3.glade:407 src/gui/siril3.glade:21340
