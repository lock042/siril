msgid "Square"
msgstr "平方"

#: src/gui/uifiles/siril.ui:7171
msgid "Point"
msgstr "点"

#: src/gui/uifiles/siril.ui:7172
msgid "Turbo"
msgstr "Turbo"

#: src/gui/uifiles/siril.ui:7173
msgid "Lanczos2"
msgstr "Lanczos2"

#: src/gui/uifiles/siril.ui:7174
msgid "Lanczos3"
msgstr "Lanczos3"

#: src/gui/uifiles/siril.ui:7186
msgid ""
